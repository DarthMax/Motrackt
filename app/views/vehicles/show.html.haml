.row
  .col-md-12
    - model_class = Vehicle
    .page-header
      %h2
        Last known position of
        =@vehicle.name

    .row
      .col-md-12
        #map

    =javascript_include_tag "https://maps.google.com/maps/api/js?sensor=false"

    %script
      $(document).ready(function() {
      window.map = new window.VehicleMap(#{@position.latitude},#{@position.longitude},#{@vehicle.id});
      });